{
    "version": 2,
    "routes": [
      { "src": "/login", "dest": "index.js" },
      { "src": "/welcome", "dest": "index.js" },
      { "src": "/logout", "dest": "index.js" },
      { "src": "/register", "dest": "index.js" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://artfulrwanda.vercel.app" },
          { "key": "Access-Control-Allow-Methods", "value": "POST, GET" },
          { "key": "Access-Control-Allow-Credentials", "value": "true" }
        ]
      }
    ]
  }
